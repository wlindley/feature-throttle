html
	head
		title Throttle Editor
	body
		h1 Throttle Editor
		form(action='/throttle-editor', method='post')
			input(type='hidden', name='redirectUri', value=redirectUri)
			table
				tr
					td Name
					td Percent
				each val, key in throttles
					tr
						td= key
						td
							input(type='text', name=key, value=val)
				tr
					td
						input(type='text', name='newName', value='')
					td
						input(type='text', name='newValue', value='')
			input(type='submit', value='Submit')
